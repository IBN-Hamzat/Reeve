{
	"name": "Reeve",
	"version": "1.1.0",
	"engines": {
		"node": "10.x"
	},
	"description": "Web application scaffolding for production environments",
	"main": "index.js",
	"scripts": {
		"start": "if-env NODE_ENV=production && npm run start:production || npm run start:development",
		"start:development": "nodemon --ignore './client/' --ignore './shared/' ./server/server",
		"start:production": "node server/server.js",
		"start:pm2": "pm2 start ecosystem.config.js",
		"build:production": "webpack -p",
		"build:start": "npm run build:production && npm run start:production",
		"test": "ava tests_integration --serial --verbose",
		"postinstall": "if-env NODE_ENV=production && npm run build:production || echo 'Bundles will build in production environment'"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/peterjoseph/reeve.git"
	},
	"keywords": [
		"node.js",
		"server",
		"node",
		"express",
		"mySQL",
		"Redis"
	],
	"author": "Peter Joseph",
	"private": true,
	"bugs": {
